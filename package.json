{
  "name": "aekino",
  "version": "1.0.0",
  "description": "WebGL render engine",
  "main": "src/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-std": "webpack --config webpack/build.config.js --progress --colors",
    "build-es6": "webpack --config webpack/build-es6.config.js --progress --colors",
    "build-min": "webpack --config webpack/build-min.config.js --progress --colors",
    "build": "npm run build-std && npm run build-min && npm run build-es6",
    "dev": "webpack-dev-server --content-base dist/ --config webpack/dev.config.js --progress --colors --watch --open"
  },
  "keywords": [
    "WebGL",
    "render",
    "engine",
    "es6",
    "2D",
    "3D"
  ],
  "author": "Damien Doussaud (namide.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namide/aekino.git"
  },
  "bugs": {
    "url": "https://github.com/namide/aekino/issues"
  },
  "homepage": "https://github.com/namide/aekino#readme",
  "dependencies": {
    "gl-matrix": "^2.3.2",
    "parse-dds": "^1.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2"
  }
}
